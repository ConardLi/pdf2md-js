{
	"name": "@tiny-tool/pdf2md",
	"version": "2.0.0",
	"repository": {
		"url": "https://github.com/tiny-tool/pdf2md"
	},
	"description": "Convert PDF to Markdown using Node.js",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
