			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "rslib build",
		"build:watch": "rslib build --watch"
	},
	"keywords": [
		"pdf",
		"markdown",
		"converter",
		"ocr"
	],
	"author": "muzea <mr.muzea@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@napi-rs/canvas": "^0.1.69",
		"@turf/turf": "^7.2.0",
		"fs-extra": "^11.3.0",
		"pdfjs-dist": "^5.1.91",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"@rslib/core": "^0.6.3",
		"@types/fs-extra": "^11.0.4",
		"@types/geojson": "^7946.0.16"
	},
	"engines": {
		"node": ">=20"
	},
	"files": [
		"dist/*"
